.panel .ui-btn {
  background-color: #000;
  color: #fff;
  padding: 10px 44px;
  margin: 0 20px;
  border-radius: 24px;
  line-height: 20px;
  text-align: center;
  font-size: 14px;
}

.panel .h1 {
  font-size: 28px;
  font-weight: 400;
  margin: 20px 0;
}

/*留白*/

.space {
  padding: 0 20px;
}

/*按钮*/

.button {
  display: block;
  padding: 14px;
  border: 2px solid #000;
  border-radius: 28px;
  font-size: 14px;
  text-align: center;
  line-height:20px;
}
.button[disabled],
.button.black {
  background-color: #000;
  color: #fff;
}

.button.white {
  background-color: #fff;
  color: #000;
}

.button.small {
  padding: 2px 10px;
  border-width: 1px;
  font-size: 12px;
}

/*设置*/
.setting-icon{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 24px;
  width: 50px;
  height: 44px;
  display: block;
  top: 0;
  left: 0;
}

