/*地址*/

.address-info {
  width: 100%;
}

.address-info .space {
  background-color: rgba(0, 0, 0, 0.03);
  padding: 20px;
  margin: 20px 20px 0 20px;
  position: relative;
}

.address-info .address-info-name text {
  font-size: 16px;
  font-weight: 500;
  margin-right: 8px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 70%;
}

.address-info .address-info-name .span {
  font-size: 12px;
  color: #d0b07a;
  border: 1px solid #d0b07a;
  border-radius: 2px;
  padding: 0 4px;
  height: 16px;
  line-height: 14px;
}

.address-info .address-info-phone {
  font-size: 16px;
  margin: 6px 0;
}

.address-info .address-info-address {
  font-size: 14px;
  color: #999;
}

.address-info .address-right-icon {
  position: absolute;
  right: 20px;
  top: 55%;
  margin-top: -10px;
}

/*订单*/

.order-item {
  background-color: rgba(0, 0, 0, 0.03);
  padding: 0 20px 20px 20px;
  margin: 0 20px 20px 20px;
}

.order-item-img {
  margin-right: 0;
}

.order-item-img image {
  width: 100px;
}

.order-item-info {
  padding-top: 20px;
  width: 100%;
}

.order-item-info .status, .order-item-info .order-item-info-t1 {
  color: #666;
  font-size: 12px;
  font-weight: 300;
}

.order-item-info .status {
  color: #d0b07a;
}

.order-item-title {
  font-weight: 400;
  font-size: 20px;
}

.order-item-time {
  color: #666;
  font-size: 12px;
  font-weight: 300;
}

.order-item-btns {
  border-top: 1px solid #eee;
  padding-top: 10px;
  margin-top: 10px;
}

.order-item-btns a {
  margin-right: 10px;
  background-color: transparent;
}

.order-item-priec {
  border-top: 1px solid #eee;
  margin-top: 10px;
  font-size: 24px;
  font-weight: 300;
}

/*没有数据*/

.no-data {
  text-align: center;
}

.no-data  image {
  width: 200px;
  display: inline-block;
}

.no-data  text {
  display: block;
  text-align: center;
}
